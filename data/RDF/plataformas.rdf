  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/ESTMAR">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Estrella de Mar</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Estrella de Mar</skos:prefLabel>
    <skos:definition xml:lang="es">Buque pesquero fresquero, perteneciente a la flota de Mar del Plata.</skos:definition>
    <skos:definition xml:lang="en">Fresh fish fishing vessel, belonging to the Mar del Plata fleet.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/FV"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/RB">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Bote pequeño</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Small boat</skos:prefLabel>
    <skos:definition xml:lang="es">Plataforma que opera sobre la superficie, que se impulsa manualmente y puede retirarse fácilmente del agua (por ejemplo, un bote de remos o una canoa).</skos:definition>
    <skos:definition xml:lang="en">Surface platform that is manually propelled and easily removable from the water (e.g., a rowboat or canoe).</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/L06/current/3A"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/SHP"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/MV">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Bote a motor</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Self-propelled small boat</skos:prefLabel>
    <skos:definition xml:lang="es">Pequeña embarcacion a motor y que se puede sacar fácilmente del agua (p. ej., semirrígidas, botes).</skos:definition>
    <skos:definition xml:lang="en">A small self-propelled platform operating on the surface of the water column that may be easily removed from the water (e.g. shore-based RIBs, ships' boats).</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="https://vocab.nerc.ac.uk/collection/L06/current/33/"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/SHP"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/OLIVIA2">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Olivia II</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Olivia II</skos:prefLabel>
    <skos:definition xml:lang="es">Buque pesquero congelador.</skos:definition>
    <skos:definition xml:lang="en">Freezer fishing vessel.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/FV"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/08L4">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Bernie</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Bernie</skos:prefLabel>
    <skos:definition xml:lang="es">Embarcación menor de Plástico Reforzado con Fibra de Vidrio (PRFV) utilizada para tareas de investigación costera por el INIDEP. Construida en 2014. Eslora: 6.68 m, Señal distintiva: LW3177.</skos:definition>
    <skos:definition xml:lang="en">Small Glass-Reinforced Plastic (GRP) boat used for coastal research tasks by INIDEP. Built in 2014. Length: 6.68 m, Call sign: LW3177.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/C17/current/08L4/"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/RV"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/08AH">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Dr. Bernardo Houssay</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Dr. Bernardo Houssay</skos:prefLabel>
    <skos:definition xml:lang="es">Motovelero oceanográfico de Argentina (CONICET). Construido en Dinamarca (1930) como "Atlantis" para la Woods Hole Oceanographic Institution, fue vendido a Argentina en 1966 y renombrado "El Austral", luego "Dr. Bernardo Houssay". Eslora: 47 m, Señal distintiva: L2EF, MMSI: 701006249.</skos:definition>
    <skos:definition xml:lang="en">Argentine oceanographic motorsailer (CONICET). Built in Denmark (1930) as "Atlantis" for the Woods Hole Oceanographic Institution, it was sold to Argentina in 1966 and renamed "El Austral," then "Dr. Bernardo Houssay." Length: 47 m, Call sign: L2EF, MMSI: 701006249.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/C17/current/08AH/"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/RV"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/PLAT">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Plataforma</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Platform</skos:prefLabel>
    <skos:definition xml:lang="es">Estructura física desde la cual se pueden realizar observaciones o mediciones.</skos:definition>
    <skos:definition xml:lang="en">Physical structure used to carry out observations or measurements.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/plataformas/AER"/>
