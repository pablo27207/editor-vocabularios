  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/SL">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">ARA San Luis</skos:prefLabel>
    <skos:prefLabel xml:lang="en">ARA San Luis</skos:prefLabel>
    <skos:definition xml:lang="es">Submarino tipo 209 de la Armada Argentina. Tuvo una destacada y reconocida participación en la Guerra de Malvinas en 1982. Fue retirado del servicio en 1997.</skos:definition>
    <skos:definition xml:lang="en">A Type 209 submarine of the Argentine Navy. It had a distinguished and recognized participation in the Malvinas War in 1982. It was retired from service in 1997.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/AR"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/9219351">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Capesante</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Capesante</skos:prefLabel>
    <skos:definition xml:lang="es">Buque pesquero congelador arrastrero, especializado en la pesca de langostino. Eslora: 42.4 m, IMO: 9219351.</skos:definition>
    <skos:definition xml:lang="en">Freezer trawler fishing vessel, specialized in shrimp fishing. Length: 42.4 m, IMO: 9219351.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/FV"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/08D8">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Victor Angelescu</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Victor Angelescu</skos:prefLabel>
    <skos:definition xml:lang="es">Buque de investigación pesquera y oceanográfica de Argentina, operado por el INIDEP. Construido en España en 2017. Eslora: 52.8 m, Señal distintiva: LW3347, IMO: 9798820, MMSI: 701114000.</skos:definition>
    <skos:definition xml:lang="en">Argentine fisheries and oceanographic research vessel, operated by INIDEP. Built in Spain in 2017. Length: 52.8 m, Call sign: LW3347, IMO: 9798820, MMSI: 701114000.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/C17/current/08D8/"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/RV"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/AR">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Embarcación de la Armada</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Naval vessel</skos:prefLabel>
    <skos:definition xml:lang="es">Embarcación que está principalmente equipada, tripulada y operada con fines militares. Incluye buques de guerra de superficie de todos los tamaños y buques de apoyo logístico.</skos:definition>
    <skos:definition xml:lang="en">Vessel primarily equipped, crewed, and operated for military purposes. Includes surface warships of all sizes and logistical support ships.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/L06/current/39"/>
