    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/plataformas/NGOES"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/8714908">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Arbumasa XVIII</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Arbumasa XVIII</skos:prefLabel>
    <skos:definition xml:lang="es">Buque pesquero potero (jigger) congelador, especializado en la pesca de calamar. Opera para la empresa Arbumasa. Eslora: 59.5 m, IMO: 8714908.</skos:definition>
    <skos:definition xml:lang="en">Freezer jigger fishing vessel, specialized in squid fishing. Operates for the Arbumasa company. Length: 59.5 m, IMO: 8714908.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/FV"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/PNAST">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Guardacostas Salvamento Tango</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Guardacostas Salvamento Tango</skos:prefLabel>
    <skos:definition xml:lang="es">Buque de salvamento y remolcador perteneciente al Servicio de Salvamento, Incendio y Protección Ambiental de la Prefectura Naval Argentina, con base en Mar del Plata.</skos:definition>
    <skos:definition xml:lang="en">Rescue and tugboat vessel belonging to the Salvage, Firefighting, and Environmental Protection Service of the Prefectura Naval Argentina, based in Mar del Plata.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/FV"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/8712477">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Catamarca</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Catamarca</skos:prefLabel>
    <skos:definition xml:lang="es">Buque pesquero congelador de la flota argentina. Eslora: 47.8 m, IMO: 8712477.</skos:definition>
    <skos:definition xml:lang="en">Freezer fishing vessel of the Argentine fleet. Length: 47.8 m, IMO: 8712477.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/FV"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/9239256">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Alvarez Entrena I</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Alvarez Entrena I</skos:prefLabel>
    <skos:definition xml:lang="es">Buque pesquero congelador arrastrero de la flota de Mar del Plata. Eslora: 38.1 m, IMO: 9239256.</skos:definition>
    <skos:definition xml:lang="en">Freezer trawler fishing vessel from the Mar del Plata fleet. Length: 38.1 m, IMO: 9239256.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/FV"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/29HE">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Hespérides</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Hespérides</skos:prefLabel>
    <skos:definition xml:lang="es">Buque de investigación oceanográfica polar de España, operado por la Armada Española. Construido en 1991, es la principal plataforma para la investigación antártica española. Eslora: 82.5 m, Señal distintiva: EBBW, IMO: 8803563.</skos:definition>
    <skos:definition xml:lang="en">Spanish polar oceanographic research vessel, operated by the Spanish Navy. Built in 1991, it is the main platform for Spanish Antarctic research. Length: 82.5 m, Call sign: EBBW, IMO: 8803563.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/C17/current/29HE"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/RV"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/33RR">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Roger Revelle</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Roger Revelle</skos:prefLabel>
    <skos:definition xml:lang="es">Buque de investigación oceanográfica de Estados Unidos, operado por la Scripps Institution of Oceanography. Ha realizado campañas de investigación en el Atlántico Sudoccidental. Eslora: 83.2 m, IMO: 9075228.</skos:definition>
    <skos:definition xml:lang="en">United States oceanographic research vessel, operated by the Scripps Institution of Oceanography. It has conducted research campaigns in the Southwest Atlantic. Length: 83.2 m, IMO: 9075228.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/C17/current/33RR"/>
