  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/RV">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Buque de investigación</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Research vessel</skos:prefLabel>
    <skos:definition xml:lang="es">Plataforma de cualquier tamaño que opera en la superficie de la columna de agua equipada, tripulada y operada específicamente para la investigación científica, generalmente oceanográfica.</skos:definition>
    <skos:definition xml:lang="en">Platform of any size operating at the sea surface, equipped, crewed, and operated specifically for scientific research, typically oceanographic.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
