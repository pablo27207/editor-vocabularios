    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Atlantico 1</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Atlantico 1</skos:prefLabel>
    <skos:definition xml:lang="es">Buque pesquero fresquero de la flota de Mar del Plata, hundido en 2013 tras un incendio. Eslora: 26.9 m.</skos:definition>
    <skos:definition xml:lang="en">Fresh fish fishing vessel from the Mar del Plata fleet, which sank in 2013 after a fire. Length: 26.9 m.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/FV"/>
