  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/08RO">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">ARA Ciudad de Rosario</skos:prefLabel>
    <skos:prefLabel xml:lang="en">ARA Ciudad de Rosario</skos:prefLabel>
    <skos:definition xml:lang="es">Aviso de la Armada Argentina, con funciones de patrullaje, balizamiento y sostenimiento logístico de otras unidades y puestos navales, especialmente en el área fluvial. Eslora: 36.6 m.</skos:definition>
    <skos:definition xml:lang="en">An "aviso" (dispatch boat) of the Argentine Navy, with patrolling, beaconing, and logistical support functions for other units and naval posts, especially in the riverine area. Length: 36.6 m.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/C17/current/08RO/"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/AR"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/LW4968">
