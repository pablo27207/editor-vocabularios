    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Dr. Eduardo Holmberg</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Dr. Eduardo Holmberg</skos:prefLabel>
    <skos:definition xml:lang="es">Buque de investigación pesquera de Argentina, operado por el INIDEP. Construido en Japón en 1980. Eslora: 61.95 m, Señal distintiva: LW 4343.</skos:definition>
    <skos:definition xml:lang="en">Argentine fisheries research vessel, operated by INIDEP. Built in Japan in 1980. Length: 61.95 m, Call sign: LW 4343.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/C17/current/08EH/"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/RV"/>
  </rdf:Description>
