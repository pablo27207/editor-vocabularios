    <skos:prefLabel xml:lang="es">Chiyo Maru 3</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Chiyo Maru 3</skos:prefLabel>
    <skos:definition xml:lang="es">Buque pesquero potero (jigger) para la pesca de calamar. Eslora: 55.6 m, IMO: 8404287.</skos:definition>
    <skos:definition xml:lang="en">Jigger fishing vessel for squid fishing. Length: 55.6 m, IMO: 8404287.</skos:definition>
