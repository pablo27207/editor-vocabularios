  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/06AQ">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Polarstern</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Polarstern</skos:prefLabel>
    <skos:definition xml:lang="es">Buque de investigación polar y rompehielos de Alemania, operado por el Instituto Alfred Wegener (AWI). Construido en 1982, es una de las embarcaciones de investigación más importantes del mundo. Eslora: 118 m, Señal distintiva: DBLK, IMO: 8013132.</skos:definition>
    <skos:definition xml:lang="en">German polar research vessel and icebreaker, operated by the Alfred Wegener Institute (AWI). Built in 1982, it is one of the most important research vessels in the world. Length: 118 m, Call sign: DBLK, IMO: 8013132.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/C17/current/06AQ/"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/RV"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/ROV">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Sumergible</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Submersible</skos:prefLabel>
    <skos:definition xml:lang="es">Plataforma no tripulada que se desplaza de forma autónoma en el medio marino, puede ser superficial o sumergible, equipada con sensores científicos.</skos:definition>
    <skos:definition xml:lang="en">Unmanned platform that moves autonomously in the marine environment, either at the surface or submerged, and is equipped with scientific sensors.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="https://vocab.nerc.ac.uk/collection/L06/current/20/ - https://mmisw.org/ont/ioos/platform/submersible"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/PLAT"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/YA">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Yamana</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Yamana</skos:prefLabel>
    <skos:definition xml:lang="es">Aviso de la Armada Argentina (A-6), utilizado para funciones de patrullaje y sostenimiento logístico de las bases en la zona austral y la Antártida. Construido en 1980. Eslora: 40 m.</skos:definition>
    <skos:definition xml:lang="en">An "aviso" (dispatch boat) of the Argentine Navy (A-6), used for patrolling and logistical support functions for bases in the southern and Antarctic zones. Built in 1980. Length: 40 m.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/AR"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/3206">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Nathaniel B. Palmer</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Nathaniel B. Palmer</skos:prefLabel>
    <skos:definition xml:lang="es">Buque de investigación polar y rompehielos de Estados Unidos, operado por el Programa Antártico de Estados Unidos (USAP). Construido en 1992. Eslora: 94 m, Señal distintiva: WBP4212, IMO: 9007257.</skos:definition>
    <skos:definition xml:lang="en">United States polar research vessel and icebreaker, operated by the United States Antarctic Program (USAP). Built in 1992. Length: 94 m, Call sign: WBP4212, IMO: 9007257.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/C17/current/3206/"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/RV"/>
  </rdf:Description>
