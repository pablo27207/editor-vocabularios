  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/FSBCH">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Estructura de playa/intermareal</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Beach/intertidal structure</skos:prefLabel>
    <skos:definition xml:lang="es">Estructura a la que se puede unir instrumentación que está en el aire o bajo el agua, según el estado de la marea y las condiciones climáticas.</skos:definition>
    <skos:definition xml:lang="en">Structure where instrumentation can be attached above or below water depending on tide and weather conditions.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/L06/current/13"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/FS"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/FSOFSHR">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Estructura offshore</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Offshore structure</skos:prefLabel>
    <skos:definition xml:lang="es">Estructura artificial fija (durante la realización de las mediciones) alejada de la costa en la que se puede instalar instrumental (por ejemplo, una plataforma petrolera, una plataforma de gas o una barcaza autoelevable).</skos:definition>
    <skos:definition xml:lang="en">Artificial structure fixed (during measurements) away from the coast where instrumentation can be installed (e.g., oil rig, gas platform, jack-up barge).</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/L06/current/16"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/FS"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/DRIFTER">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Flotador a la deriva</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Drifting float</skos:prefLabel>
    <skos:definition xml:lang="es">Plataforma que flota libremente ya sea en la superficie de la columna de agua o a una profundidad predeterminada dentro de la columna de agua.</skos:definition>
    <skos:definition xml:lang="en">Platform that floats freely either at the surface or at a predefined depth in the water column.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/L06/current/47/"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/FLOAT"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/SAT">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Satélite</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Satellite</skos:prefLabel>
    <skos:definition xml:lang="es">Plataforma no tripulada que orbita fuera de la atmósfera terrestre, equipada con sensores de observación remota.</skos:definition>
    <skos:definition xml:lang="en">Unmanned platform orbiting beyond Earth’s atmosphere, equipped with remote sensing instruments.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/L06/current/68/"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/PLAT"/>
