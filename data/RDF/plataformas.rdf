    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Don Julio</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Don Julio</skos:prefLabel>
    <skos:definition xml:lang="es">Buque pesquero fresquero de altura, perteneciente a la flota de Mar del Plata. Eslora: 27.5 m, Señal distintiva: LW4968.</skos:definition>
    <skos:definition xml:lang="en">Fresh fish trawler (deep-sea), part of the Mar del Plata fleet. Length: 27.5 m, Call sign: LW4968.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/FV"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/ATLANT1">
