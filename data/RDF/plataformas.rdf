    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/RV"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/PFLOAT">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Flotador perfilado</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Profiling float</skos:prefLabel>
    <skos:definition xml:lang="es">Plataforma que se desplaza libremente en la columna de agua y que periódicamente realiza recorridos verticales a través de la misma (por ejemplo, el flotador Argo).</skos:definition>
    <skos:definition xml:lang="en">Freely moving platform in the water column that periodically performs vertical profiles (e.g., Argo float).</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/L06/current/46/"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/FLOAT"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/DIV">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Buzo</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Diver</skos:prefLabel>
    <skos:definition xml:lang="es">Un ser humano con equipo autónomo o traje conectado a la superficie que permite operar dentro de la columna de agua.</skos:definition>
    <skos:definition xml:lang="en">A human equipped with autonomous or surface-linked gear enabling operation within the water column.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/L06/current/72/"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/PLAT"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/31AN">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Atlantis II</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Atlantis II</skos:prefLabel>
    <skos:definition xml:lang="es">Buque de investigación de Estados Unidos, operado por la Woods Hole Oceanographic Institution (WHOI) entre 1963 y 1996. Fue el buque nodriza del submarino Alvin, con el que se descubrieron los restos del Titanic. Eslora: 64 m, Señal distintiva: KADC.</skos:definition>
    <skos:definition xml:lang="en">United States research vessel, operated by the Woods Hole Oceanographic Institution (WHOI) between 1963 and 1996. It was the mother ship for the submersible Alvin, with which the wreck of the Titanic was discovered. Length: 64 m, Call sign: KADC.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/C17/current/31AN"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/RV"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/FSTERR">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Estructura en tierra</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Terrestrial structure</skos:prefLabel>
    <skos:definition xml:lang="es">Estructura fija construida por el hombre en tierra a la que se puede conectar instrumentación (por ejemplo, una torre meteorológica).</skos:definition>
    <skos:definition xml:lang="en">Man-made fixed structure on land where instrumentation can be mounted (e.g., a meteorological tower).</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/L06/current/14"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/FS"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/VOO">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Buque de oportunidad</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Vessel of opportunity</skos:prefLabel>
    <skos:definition xml:lang="es">Embarcación comercial de cualquier tamaño que recopila regularmente datos científicos (oceanográficos y meteorológicos).</skos:definition>
    <skos:definition xml:lang="en">Commercial vessel of any size that regularly collects scientific data (oceanographic and meteorological).</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/L06/current/32"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/SHP"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/8712489">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Patagonia</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Patagonia</skos:prefLabel>
    <skos:definition xml:lang="es">Buque pesquero congelador, perteneciente a la flota de Mar del Plata. Eslora: 47.8 m, IMO: 8712489.</skos:definition>
    <skos:definition xml:lang="en">Freezer fishing vessel, belonging to the Mar del Plata fleet. Length: 47.8 m, IMO: 8712489.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/FV"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/18OL">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Coriolis II</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Coriolis II</skos:prefLabel>
    <skos:definition xml:lang="es">Buque de investigación de Canadá, operado por un consorcio liderado por la Universidad de Quebec en Rimouski (UQAR). Construido en 1991, fue anteriormente el buque "CCGS 501" de la Guardia Costera Canadiense. Eslora: 50 m, Señal distintiva: VFNQ, IMO: 8713837.</skos:definition>
    <skos:definition xml:lang="en">Canadian research vessel, operated by a consortium led by the University of Quebec at Rimouski (UQAR). Built in 1991, it was formerly the Canadian Coast Guard ship "CCGS 501". Length: 50 m, Call sign: VFNQ, IMO: 8713837.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/C17/current/18OL"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/RV"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/GZ">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">General Zapiola</skos:prefLabel>
    <skos:prefLabel xml:lang="en">General Zapiola</skos:prefLabel>
    <skos:definition xml:lang="es">Fue una lancha hidrográfica de la Armada Argentina, destinada a realizar levantamientos y estudios en las costas y ríos del país.</skos:definition>
    <skos:definition xml:lang="en">It was a hydrographic launch of the Argentine Navy, assigned to conduct surveys and studies along the country's coasts and rivers.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/AR"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/9005952">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Centurion del Atlantico</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Centurion del Atlantico</skos:prefLabel>
    <skos:definition xml:lang="es">Buque pesquero congelador arrastrero, operado por la empresa Antártida Pesquera. Eslora: 53.7 m, IMO: 9005952.</skos:definition>
    <skos:definition xml:lang="en">Freezer trawler fishing vessel, operated by the company Antártida Pesquera. Length: 53.7 m, IMO: 9005952.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/FV"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/08P5">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Willy</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Willy</skos:prefLabel>
    <skos:definition xml:lang="es">Embarcación menor de Plástico Reforzado con Fibra de Vidrio (PRFV) utilizada para tareas de investigación costera por el INIDEP. Construida en 2014. Eslora: 6.68 m, Señal distintiva: LW3176.</skos:definition>
    <skos:definition xml:lang="en">Small Glass-Reinforced Plastic (GRP) boat used for coastal research tasks by INIDEP. Built in 2014. Length: 6.68 m, Call sign: LW3176.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/C17/current/08P5/"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/RV"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/8846064">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Erin Bruce</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Erin Bruce</skos:prefLabel>
    <skos:definition xml:lang="es">Buque pesquero congelador, opera en el Atlántico Sur con base en el puerto de Mar del Plata. Eslora: 47.7 m, IMO: 8846064.</skos:definition>
    <skos:definition xml:lang="en">Freezer fishing vessel, operates in the South Atlantic based in the port of Mar del Plata. Length: 47.7 m, IMO: 8846064.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/FV"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/FSBUOY">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Boya</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Moored platform</skos:prefLabel>
    <skos:definition xml:lang="es">Plataforma amarrada de forma flexible al fondo del mar para mantener una posición fija que puede incluir instrumentos oceanográficos con componentes en el fondo marino, en aguas intermedias y en superficie.</skos:definition>
    <skos:definition xml:lang="en">Flexibly moored platform anchored to the seabed to maintain a fixed position, potentially including oceanographic instruments at the seabed, mid-water, and surface.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/L06/current/48/ - https://mmisw.org/ont/ioos/platform/buoy"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/FS"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/FLOAT">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Flotador</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Float</skos:prefLabel>
    <skos:definition xml:lang="es">Plataforma de observación autónoma que flota en la superficie o en una profundidad determinada.</skos:definition>
    <skos:definition xml:lang="en">Autonomous observation platform floating either at the surface or at a predetermined depth.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/L06/current/47/"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/PLAT"/>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/plataformas/DRIFTER"/>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/plataformas/PFLOAT"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/FSCOAST">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Estructura costera</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Coastal structure</skos:prefLabel>
    <skos:definition xml:lang="es">Estructura fija permanentemente unida a la tierra, con acceso al agua en todos los estados de la marea, en la que se puede instalar instrumentos de medición (por ejemplo, un muelle).</skos:definition>
    <skos:definition xml:lang="en">Permanently fixed structure attached to land with access to water at all tide levels, suitable for installing measuring instruments (e.g., a pier).</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/L06/current/17"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/FS"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/ATLEXP">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Atlantic Express</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Atlantic Express</skos:prefLabel>
    <skos:definition xml:lang="es">Barco pesquero.</skos:definition>
    <skos:definition xml:lang="en">Fishing vessel.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/FV"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/06WH">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Walther Herwig III</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Walther Herwig III</skos:prefLabel>
    <skos:definition xml:lang="es">Buque de investigación pesquera de Alemania. La versión III fue construida en 1993 y es operada por el Instituto Thünen. Eslora: 64.5 m, Señal distintiva: DBFX, IMO: 9059535.</skos:definition>
    <skos:definition xml:lang="en">German fisheries research vessel. Version III was built in 1993 and is operated by the Thünen Institute. Length: 64.5 m, Call sign: DBFX, IMO: 9059535.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/C17/current/06WH"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/RV"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/TALISM">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Talisman</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Talisman</skos:prefLabel>
    <skos:definition xml:lang="es">Buque pesquero fresquero de la flota de Mar del Plata. Eslora: 22 m.</skos:definition>
    <skos:definition xml:lang="en">Fresh fish fishing vessel from the Mar del Plata fleet. Length: 22 m.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/FV"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/8609556">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Dukat</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Dukat</skos:prefLabel>
    <skos:definition xml:lang="es">Buque pesquero congelador, perteneciente a la flota de Mar del Plata. Eslora: 50.4 m, IMO: 8609556.</skos:definition>
    <skos:definition xml:lang="en">Freezer fishing vessel, belonging to the Mar del Plata fleet. Length: 50.4 m, IMO: 8609556.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/FV"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/9634932">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Miss Tide</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Miss Tide</skos:prefLabel>
    <skos:definition xml:lang="es">Buque de apoyo para la industria petrolera y del gas (Supply Tender). Si bien no es pesquero, opera frecuentemente desde el puerto de Mar del Plata. Eslora: 45.7 m, IMO: 9634932.</skos:definition>
    <skos:definition xml:lang="en">Offshore supply tender vessel for the oil and gas industry. While not a fishing vessel, it frequently operates from the port of Mar del Plata. Length: 45.7 m, IMO: 9634932.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/FV"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/FS">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Estación fija</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Fixed station</skos:prefLabel>
    <skos:definition xml:lang="es">Instalación inmóvil utilizada como base de observación o monitoreo, ya sea en tierra, costa o mar.</skos:definition>
    <skos:definition xml:lang="en">Immobile facility used as a base for observation or monitoring, located on land, shore, or at sea.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/L06/current/17/"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/PLAT"/>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/plataformas/FSCOAST"/>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/plataformas/FSBCH"/>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/plataformas/FSTERR"/>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/plataformas/FSOFSHR"/>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/plataformas/FSBUOY"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/SHP">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Buque - Embarcación</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Ship</skos:prefLabel>
    <skos:definition xml:lang="es">Plataforma que opera sobre la superficie del mar, destinada al transporte, actividades comerciales o científicas.</skos:definition>
    <skos:definition xml:lang="en">Platform operating at the sea surface, used for transport, commercial, or scientific purposes.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/L06/current/30/"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/PLAT"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/ML">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">ROU Maldonado</skos:prefLabel>
    <skos:prefLabel xml:lang="en">ROU Maldonado</skos:prefLabel>
    <skos:definition xml:lang="es">Buque hidrográfico de la Armada de Uruguay (ROU). La unidad ROU 23, comisionada en 2000, fue construida en Alemania y transferida a Uruguay para tareas de investigación.</skos:definition>
    <skos:definition xml:lang="en">Hydrographic vessel of the Uruguayan Navy (ROU). Unit ROU 23, commissioned in 2000, was built in Germany and transferred to Uruguay for research tasks.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/AR"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/GLIDER">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Glider</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Glider</skos:prefLabel>
    <skos:definition xml:lang="es">Plataforma autónoma que se desplaza por la columna de agua mediante cambios de flotabilidad y alas, permitiendo misiones prolongadas con bajo consumo de energía.</skos:definition>
    <skos:definition xml:lang="en">Autonomous platform that moves through the water column using buoyancy changes and wings, enabling long-duration, low-power missions.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="https://vocab.nerc.ac.uk/collection/L06/current/6A/"/>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/PLAT"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/9201994">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">Cabo Buena Esperanza</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Cabo Buena Esperanza</skos:prefLabel>
    <skos:definition xml:lang="es">Buque pesquero congelador, operado por la empresa Iberconsa de Argentina. Eslora: 54.7 m, IMO: 9201994.</skos:definition>
    <skos:definition xml:lang="en">Freezer fishing vessel, operated by the company Iberconsa de Argentina. Length: 54.7 m, IMO: 9201994.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/plataformas/FV"/>
