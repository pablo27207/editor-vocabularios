  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/plataformas/08PD">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/plataformas/"/>
    <skos:prefLabel xml:lang="es">ARA Puerto Deseado</skos:prefLabel>
    <skos:prefLabel xml:lang="en">ARA Puerto Deseado</skos:prefLabel>
    <skos:definition xml:lang="es">Buque oceanográfico de la Armada Argentina, operado por el CONICET. Construido en Argentina en 1971. Eslora: 76.8 m, Señal distintiva: LOPD.</skos:definition>
    <skos:definition xml:lang="en">Oceanographic vessel of the Argentine Navy, operated by CONICET. Built in Argentina in 1971. Length: 76.8 m, Call sign: LOPD.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/C17/current/08PD/"/>
