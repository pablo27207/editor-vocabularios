  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/muestreadores/CTD">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/muestreadores/"/>
    <skos:prefLabel xml:lang="es">CTD</skos:prefLabel>
    <skos:prefLabel xml:lang="en">CTD</skos:prefLabel>
    <skos:definition xml:lang="es">Instrumento que mide simultáneamente la conductividad eléctrica del agua de mar, su temperatura y la presión en la columna de agua. Permite la integración de diferentes sensores auxiliares (pH, oxígeno disuelto, turbidez, fluorescencia)</skos:definition>
    <skos:definition xml:lang="en">Instrument that simultaneously measures the electrical conductivity of seawater, its temperature, and the pressure in the water column. It allows the integration of different auxiliary sensors (pH, dissolved oxygen, turbidity, fluorescence).</skos:definition>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/L05/current/130"/>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/muestreadores/TOOL0042"/>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/muestreadores/TOOL0058"/>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/muestreadores/TOOL0040"/>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/muestreadores/TOOL0149"/>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/muestreadores/TRCITADEL"/>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/muestreadores/TOOL0047"/>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/muestreadores/TOOL0002"/>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/muestreadores/TOOL1883"/>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/muestreadores/TOOL1162"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/muestreadores/AGEAR">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/muestreadores/"/>
    <skos:prefLabel xml:lang="es">Equipo de pesca activa</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Active fishing gear</skos:prefLabel>
    <skos:definition xml:lang="es">Dispositivos de pesca en los que la captura se basa en la persecución de la especie objetivo (ej. red de arrastre, dragas )</skos:definition>
    <skos:definition xml:lang="en">Fishing devices in which the capture is based on the pursuit of the target species (e.g., trawl net, dredges).</skos:definition>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/L05/current/64"/>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/muestreadores/NET"/>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/muestreadores/DREDGE"/>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/muestreadores/TOOL1575"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/muestreadores/NETT0024">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/muestreadores/"/>
    <skos:prefLabel xml:lang="es">Red cónica tipo Motoda</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Motoda horizontal closing plankton net</skos:prefLabel>
    <skos:definition xml:lang="es">Red de plancton con cierre y división horizontal.</skos:definition>
    <skos:definition xml:lang="en">A horizontally-divided closing plankton net.</skos:definition>
    <skos:exactMatch rdf:resource="https://vocab.nerc.ac.uk/collection/L22/current/NETT0024/"/>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/muestreadores/PNETS"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/muestreadores/TOOL1575">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/muestreadores/"/>
    <skos:prefLabel xml:lang="es">Caña - línea</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Handlines and pole-lines</skos:prefLabel>
    <skos:definition xml:lang="es">Aparato de pesca que consiste en una línea con uno o más anzuelos que se maneja manualmente.</skos:definition>
    <skos:definition xml:lang="en">Fishing gear consisting of a line with one or more hooks operated by hand.</skos:definition>
    <skos:exactMatch rdf:resource="https://vocab.nerc.ac.uk/collection/L22/current/TOOL1575/"/>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/muestreadores/AGEAR"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/muestreadores/TRAP">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/muestreadores/"/>
    <skos:prefLabel xml:lang="es">Trampa</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Trap</skos:prefLabel>
    <skos:definition xml:lang="es">Dispositivo pasivo diseñado para que los organismos puedan entrar pero no salir fácilmente.</skos:definition>
    <skos:definition xml:lang="en">A passive device designed for organisms to enter but not easily exit.</skos:definition>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/muestreadores/PGEAR"/>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/muestreadores/POT"/>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/muestreadores/TNETS"/>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/muestreadores/GNETS"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/muestreadores/NETT0071">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/muestreadores/"/>
    <skos:prefLabel xml:lang="es">Red Isaacs-Kidd (IKMT)</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Isaacs-Kidd midwater trawl net</skos:prefLabel>
    <skos:definition xml:lang="es">Red de arrastre pelágica de media agua diseñada para recolectar organismos a altas velocidades de remolque.</skos:definition>
    <skos:definition xml:lang="en">A mid-water pelagic trawl designed to collect organisms at high towing speeds.</skos:definition>
    <skos:altLabel xml:lang="en">IKMT</skos:altLabel>
    <skos:exactMatch rdf:resource="https://vocab.nerc.ac.uk/collection/L22/current/NETT0071/"/>
    <skos:closeMatch rdf:resource="https://vocab.nerc.ac.uk/collection/L22/current/NETT0073/"/>
    <skos:closeMatch rdf:resource="https://vocab.nerc.ac.uk/collection/L22/current/NETT0072/"/>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/muestreadores/PNETS"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/muestreadores/BATHY">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/muestreadores/"/>
    <skos:prefLabel xml:lang="es">Batitermografos</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Bathythermograph</skos:prefLabel>
    <skos:definition xml:lang="es">Instrumento que mide perfiles verticales de temperatura del mar (MBT, XBT).</skos:definition>
    <skos:definition xml:lang="en">Instrument that measures vertical profiles of sea temperature (MBT, XBT).</skos:definition>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/L05/current/132"/>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/muestreadores/JIG">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/muestreadores/"/>
    <skos:prefLabel xml:lang="es">Poteras</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Jigs</skos:prefLabel>
    <skos:definition xml:lang="es">Tipo de señuelo de pesca que consiste en un anzuelo con peso. Se utiliza con un movimiento de sube y baja para la pesca de calamar.</skos:definition>
    <skos:definition xml:lang="en">A type of fishing lure consisting of a weighted hook. It is used with a jerking, up-and-down motion for squid fishing.</skos:definition>
    <skos:altLabel xml:lang="en">Jiggers</skos:altLabel>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/muestreadores/FISHNET"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/muestreadores/TOOL0994">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/muestreadores/"/>
    <skos:prefLabel xml:lang="es">Red Mini-Bongo</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Mini-bongo net</skos:prefLabel>
    <skos:definition xml:lang="es">Versión pequeña de la red bongo.</skos:definition>
    <skos:definition xml:lang="en">Small version of the bongo net.</skos:definition>
    <skos:exactMatch rdf:resource="https://vocab.nerc.ac.uk/collection/L22/current/TOOL0994/"/>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/muestreadores/PNETS"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/muestreadores/CAM">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/muestreadores/"/>
    <skos:prefLabel xml:lang="es">Camara fotográfica</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Camera</skos:prefLabel>
    <skos:definition xml:lang="es">Todo tipo de equipo fotográfico utilizado para registrar imágenes visuales. Incluye fotografías, videos, películas y sistemas digitales.</skos:definition>
    <skos:definition xml:lang="en">All types of photographic equipment used to record visual images. Includes photographs, videos, films, and digital systems.</skos:definition>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/L05/current/311"/>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/muestreadores/MNET">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/muestreadores/"/>
    <skos:prefLabel xml:lang="es">Multired</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Multinet</skos:prefLabel>
    <skos:definition xml:lang="es">Sistema que comprende varias redes que pueden abrirse y cerrarse secuencialmente para recolectar una serie de muestras (microzooplancton, mesozooplancton y nekton) durante un remolque o arrastre.</skos:definition>
    <skos:definition xml:lang="en">System comprising several nets that can be opened and closed sequentially to collect a series of samples (microzooplankton, mesozooplankton, and nekton) during one tow or trawl.</skos:definition>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/L05/current/68"/>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/muestreadores/PNETS"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/muestreadores/NETT0054">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/muestreadores/"/>
    <skos:prefLabel xml:lang="es">Hensen</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Hensen net</skos:prefLabel>
    <skos:definition xml:lang="es">Red de plancton vertical con una sección superior cónica de lona y una sección inferior de malla filtrante.</skos:definition>
    <skos:definition xml:lang="en">A vertical plankton net with a conical canvas upper section and a filtering mesh lower section.</skos:definition>
    <skos:exactMatch rdf:resource="https://vocab.nerc.ac.uk/collection/L22/current/NETT0054/"/>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/muestreadores/PNETS"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/muestreadores/PTNET">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/muestreadores/"/>
    <skos:prefLabel xml:lang="es">Red de arrastre pelágica</skos:prefLabel>
    <skos:prefLabel xml:lang="en">pelagic trawl nets</skos:prefLabel>
    <skos:definition xml:lang="es">Red remolcada a través de la columna de agua diseñada para muestrear peces o nécton que nadan libremente.</skos:definition>
    <skos:definition xml:lang="en">A net towed through the water column designed to sample free-swimming nekton or fish.</skos:definition>
    <skos:exactMatch rdf:resource="https://vocab.nerc.ac.uk/collection/L05/current/23/"/>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/muestreadores/FISHNET"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/muestreadores/TOOL0267">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/muestreadores/"/>
    <skos:prefLabel xml:lang="es">STAR ODDI Starmon mini</skos:prefLabel>
    <skos:prefLabel xml:lang="en">STAR ODDI Starmon mini</skos:prefLabel>
    <skos:definition xml:lang="es">Un registrador de temperatura submarino en una carcasa de plástico (400 m) o de titanio (11000 m). La resolución promedio de la temperatura es de 0.013 °C y la precisión es de ±0.05 °C en un rango de -2 °C a 40 °C; la precisión del reloj es mejor que 1 minuto por mes. Equilibración de la temperatura: 63% en 18 segundos (carcasa de plástico) o 6 segundos (carcasa de titanio); 100% en 3 minutos (carcasa de plástico) o 1 minuto (carcasa de titanio).</skos:definition>
    <skos:definition xml:lang="en">An underwater temperature recorder in either a plastic (400m) or titanium (11000m) casing. Temperature average resolution is 0.013C and accuracy is +-0.05C over a range -2C to 40C: clock accuray better than 1 minute per month. Temperature equilibration: 63% 18 seconds (plastic case) or 6 seconds (titanium case); 100% 3 minutes (plastic case) or 1 minute (titanium case).</skos:definition>
    <skos:exactMatch rdf:resource="https://vocab.nerc.ac.uk/collection/L22/current/TOOL0267/"/>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/muestreadores/TEMP"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/muestreadores/TSAL">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/muestreadores/"/>
    <skos:prefLabel xml:lang="es">Termosalinografo</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Thermosalinograph</skos:prefLabel>
    <skos:definition xml:lang="es">Instrumento con sensores de temperatura y conductividad, instalado cerca de una toma de agua en una embarcación, para medir en continuo la capa superficial del agua.</skos:definition>
    <skos:definition xml:lang="en">Instrument with temperature and conductivity sensors, installed near a water intake on a vessel, to continuously measure the surface water layer.</skos:definition>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/L05/current/133"/>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/muestreadores/TOOL0190"/>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/muestreadores/TOOL0667"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/muestreadores/CPROF">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/muestreadores/"/>
    <skos:prefLabel xml:lang="es">Perfilador de corriente</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Current profiler</skos:prefLabel>
    <skos:definition xml:lang="es">Instrumento que mide la velocidad y la dirección de la corriente a múltiples profundidades predeterminadas simultáneamente (Ej. ADCP).</skos:definition>
    <skos:definition xml:lang="en">Instrument that measures the speed and direction of the current at multiple predetermined depths simultaneously (e.g., ADCP).</skos:definition>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/L05/current/115"/>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/muestreadores/TOOL0062"/>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/muestreadores/TOOL0061"/>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/muestreadores/KMEC150"/>
    <skos:narrower rdf:resource="https://vocab.inidep.edu.ar/muestreadores/NAQP400"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/muestreadores/TOOL0191">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/muestreadores/"/>
    <skos:prefLabel xml:lang="es">Termómetro Sea-Bird SBE 38</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Sea-Bird SBE 38 thermometer</skos:prefLabel>
    <skos:definition xml:lang="es">Un termómetro oceanográfico con un rango de medición de -5 a +35 °C; precisión absoluta mejor que 0.001 °C y resolución de aproximadamente 0.00025 °C. El sensor es un termistor envejecido ultraestable con una tasa de deriva de menos de 0.002 °C por año. La resistencia de referencia es un VISHAY sellado herméticamente. Puede ser instalado en una carcasa de presión de titanio para 10,500 m.</skos:definition>
    <skos:definition xml:lang="en">An oceanographic thermometer with a measurement range of -5 to +35C; absolute accuracy better than 0.001C and resolution of approximately 0.00025C. The sensor is an ultra-stable aged thermistor with a drift rate of less than 0.002C per year. The reference resistor is a hermetically sealed VISHAY. It may be fitted in a 10,500m titanium pressure housing.</skos:definition>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/L22/current/TOOL0191/"/>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/muestreadores/TEMP"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/muestreadores/SVEL">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/muestreadores/"/>
    <skos:prefLabel xml:lang="es">Sensores de velocidad del sonido</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Sound velocity sensors</skos:prefLabel>
    <skos:definition xml:lang="es">Instrumento que mide la velocidad del sonido en el agua de mar</skos:definition>
    <skos:definition xml:lang="en">Instrument that measures the speed of sound in seawater.</skos:definition>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/L05/current/185"/>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/muestreadores/TRCITADEL">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/muestreadores/"/>
    <skos:prefLabel xml:lang="es">CTD-NV Teledyne RD CITADEL</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Teledyne RD CITADEL CTD-NV</skos:prefLabel>
    <skos:definition xml:lang="es">Un CTD (Conductividad, Temperatura, Profundidad) robusto y de muestreo rápido, diseñado para mediciones de precisión en entornos marinos exigentes. Utiliza una celda de conductividad inductiva no externa (NXIC) patentada, lo que lo hace duradero y adecuado para perfiles, remolques o para ser montado en AUVs y ROVs.</skos:definition>
    <skos:definition xml:lang="en">A rugged, fast-sampling CTD (Conductivity, Temperature, Depth) designed for precision measurement in demanding marine environments. It utilizes a patented Non-eXternal Inductive Cell (NXIC) for conductivity, making it durable and well-suited for profiling, towing, or mounting on AUVs and ROVs.</skos:definition>
    <skos:closeMatch rdf:resource="http://vocab.nerc.ac.uk/collection/L22/current/TOOL1163/"/>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/muestreadores/CTD"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/muestreadores/WSTAT">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/muestreadores/"/>
    <skos:prefLabel xml:lang="es">Estacion meteorologica</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Weather station</skos:prefLabel>
    <skos:definition xml:lang="es">Instrumento que realiza mediciones meteorológicas de rutina en la atmósfera, normalmente la presión del aire, la temperatura y la humedad.</skos:definition>
    <skos:definition xml:lang="en">Instrument that performs routine meteorological measurements in the atmosphere, typically air pressure, temperature, and humidity.</skos:definition>
    <skos:exactMatch rdf:resource="http://vocab.nerc.ac.uk/collection/L05/current/102"/>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/muestreadores/TNETS">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/muestreadores/"/>
    <skos:prefLabel xml:lang="es">Trampa de Peces</skos:prefLabel>
    <skos:prefLabel xml:lang="en">trap nets</skos:prefLabel>
    <skos:definition xml:lang="es">Redes estacionarias fijadas al fondo que consisten en un laberinto en el que los peces pueden entrar fácilmente, pero del que no pueden escapar.</skos:definition>
    <skos:definition xml:lang="en">Stationary nets fixed to the bottom, consisting of a labyrinth-like chamber into which fish can easily enter and from which they cannot easily escape.</skos:definition>
    <skos:altLabel xml:lang="en">Fish trap</skos:altLabel>
    <skos:exactMatch rdf:resource="https://vocab.nerc.ac.uk/collection/L05/current/74/"/>
    <dc:source>Gabinete de oceanografía fisica, INIDEP.</dc:source>
    <skos:broader rdf:resource="https://vocab.inidep.edu.ar/muestreadores/TRAP"/>
  </rdf:Description>
