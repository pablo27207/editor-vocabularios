    <skos:altLabel xml:lang="en">LDEO</skos:altLabel>
    <dc:source>https://www.ldeo.columbia.edu/</dc:source>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/organizaciones/COI">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/organizaciones/"/>
