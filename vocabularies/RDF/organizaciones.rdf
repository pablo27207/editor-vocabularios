    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/organizaciones/"/>
    <skos:prefLabel xml:lang="es">Universidad de Buenos Aires</skos:prefLabel>
    <skos:prefLabel xml:lang="en">University of Buenos Aires</skos:prefLabel>
    <skos:definition xml:lang="es">Principal universidad pública de Argentina, con una amplia gama de programas de investigación, incluyendo ciencias del mar.</skos:definition>
    <skos:definition xml:lang="en">The main public university in Argentina, with a wide range of research programs, including marine sciences.</skos:definition>
    <skos:altLabel xml:lang="en">UBA</skos:altLabel>
    <dc:source>https://www.uba.ar/</dc:source>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/organizaciones/CIMAS">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/organizaciones/"/>
    <skos:prefLabel xml:lang="es">Centro de Investigación Aplicada y Transferencia Tecnológica en Recursos Marinos Alte. Storni</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Alte. Storni Center for Applied Research and Technology Transfer in Marine Resources</skos:prefLabel>
    <skos:definition xml:lang="es">Instituto de investigación en San Antonio Oeste, Argentina, enfocado en la acuicultura y los recursos marinos patagónicos.</skos:definition>
    <skos:definition xml:lang="en">Research institute in San Antonio Oeste, Argentina, focused on aquaculture and Patagonian marine resources.</skos:definition>
    <skos:altLabel xml:lang="en">CIMAS</skos:altLabel>
    <dc:source>https://cimas.org.ar/</dc:source>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/organizaciones/IAA">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/organizaciones/"/>
    <skos:prefLabel xml:lang="es">Instituto Antártico Argentino</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Argentine Antarctic Institute</skos:prefLabel>
    <skos:definition xml:lang="es">Organismo gubernamental de Argentina que centraliza la planificación y ejecución de la investigación científica en la Antártida.</skos:definition>
    <skos:definition xml:lang="en">Governmental body of Argentina that centralizes the planning and execution of scientific research in Antarctica.</skos:definition>
    <skos:altLabel xml:lang="en">IAA</skos:altLabel>
    <dc:source>https://www.argentina.gob.ar/ciencia/iaa</dc:source>
  </rdf:Description>
