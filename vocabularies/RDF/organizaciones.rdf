    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/organizaciones/"/>
    <skos:prefLabel xml:lang="es">Organización de las Naciones Unidas para la Alimentación y la Agricultura</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Food and Agriculture Organization of the United Nations</skos:prefLabel>
