    <skos:definition xml:lang="en">Public body in Argentina dedicated to the scientific and technical research of fishery resources and the marine environment.</skos:definition>
    <skos:altLabel xml:lang="en">INIDEP</skos:altLabel>
    <dc:source>https://www.argentina.gob.ar/inidep</dc:source>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/organizaciones/CADIC">
