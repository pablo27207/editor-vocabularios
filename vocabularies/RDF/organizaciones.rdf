    <skos:definition xml:lang="en">Governmental body at the national or provincial level responsible for environmental and sustainable development policies.</skos:definition>
    <skos:altLabel xml:lang="en">SAyDS</skos:altLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/organizaciones/LDGO">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/organizaciones/"/>
