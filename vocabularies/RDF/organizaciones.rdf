    <dc:source>https://www.fao.org/</dc:source>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/organizaciones/USP">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/organizaciones/"/>
    <skos:prefLabel xml:lang="es">Universidade de São Paulo</skos:prefLabel>
    <skos:prefLabel xml:lang="en">University of São Paulo</skos:prefLabel>
    <skos:definition xml:lang="es">Una de las universidades públicas más importantes de Brasil, con un prestigioso Instituto Oceanográfico.</skos:definition>
    <skos:definition xml:lang="en">One of the most important public universities in Brazil, with a prestigious Oceanographic Institute.</skos:definition>
    <skos:altLabel xml:lang="en">USP</skos:altLabel>
    <dc:source>https://www5.usp.br/</dc:source>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/organizaciones/SAYDS">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/organizaciones/"/>
    <skos:prefLabel xml:lang="es">Secretaría de Ambiente y Desarrollo Sustentable</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Secretariat of Environment and Sustainable Development</skos:prefLabel>
