  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/organizaciones/CENPAT">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://vocab.inidep.edu.ar/organizaciones/"/>
    <skos:prefLabel xml:lang="es">Centro Nacional Patagónico -CONICET</skos:prefLabel>
    <skos:prefLabel xml:lang="en">National Patagonian Center -CONICET</skos:prefLabel>
    <skos:definition xml:lang="es">Centro de investigación multidisciplinario del CONICET en Puerto Madryn, enfocado en ecosistemas marinos y terrestres de la Patagonia.</skos:definition>
