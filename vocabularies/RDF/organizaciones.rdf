    <dc:source>https://ioc.unesco.org/</dc:source>
  </rdf:Description>
  <rdf:Description rdf:about="https://vocab.inidep.edu.ar/organizaciones/CIMA">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
